<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("//code.jquery.com/mobile/1.4.0/jquery.mobile-1.4.0.min.css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("//code.jquery.com/mobile/1.4.0/jquery.mobile-1.4.0.min.js")
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
