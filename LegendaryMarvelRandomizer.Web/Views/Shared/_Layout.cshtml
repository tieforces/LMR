<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css")
    @Styles.Render("//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap-theme.min.css")
    @Styles.Render("//code.jquery.com/mobile/1.4.0/jquery.mobile-1.4.0.min.css")
    @Styles.Render("~/Content/bootstrap-select.min.css")
    @Scripts.Render("//code.jquery.com/jquery-2.1.1.min.js")
    @Scripts.Render("~/bundles/modernizr")
    @*@Scripts.Render("//code.jquery.com/mobile/1.4.0/jquery.mobile-1.4.0.min.js")*@
    @Scripts.Render("//netdna.bootstrapcdn.com/bootstrap/3.1.0/js/bootstrap.min.js")
    @Scripts.Render("~/Scripts/jquery.validate.min.js")
    @Scripts.Render("~/Scripts/jquery.validate.unobtrusive.min.js")
    @Scripts.Render("~/Scripts/jqBootstrapValidation.js")
    @Scripts.Render("~/Scripts/bootstrap-select.min.js")
    @Scripts.Render("~/Scripts/knockout-3.1.0.js")
    @Scripts.Render("~/Scripts/knockout.mapping-latest.js")
    @Scripts.Render("~/Scripts/moment.js")
    @Scripts.Render("~/Scripts/randomizer.js")
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
